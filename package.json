{
  "name": "cultivate-uw-ml-mvp",
  "version": "1.0.0",
  "description": "Machine Learning MVP for analyzing educator-child interactions in early childhood education",
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "serve": "./scripts/serve.sh",
    "dev": "npm run serve",
    "start": "npm run serve",
    "test": "npm run test:api && npm run test:scenarios",
    "test:api": "python3 test_api_comprehensive.py",
    "test:scenarios": "python3 verify_demo2_system.py",
    "lint": "npm run lint:python",
    "lint:python": "cd src && flake8 . && black --check .",
    "format": "npm run format:python",
    "format:python": "cd src && black .",
    "setup": "python3 -m venv venv && venv/bin/pip install -r requirements.txt && mkdir -p logs",
    "clean": "rm -rf venv/__pycache__ src/**/__pycache__ logs/*.log",
    "clean:all": "npm run clean && rm -rf venv",
    "docker:build": "docker build -t cultivate-uw-ml .",
    "docker:run": "docker run -p 3000:3000 -p 5001:5001 cultivate-uw-ml"
  },
  "keywords": [
    "machine-learning",
    "early-education",
    "educator-training",
    "constructivist-learning",
    "research",
    "ai",
    "computer-vision",
    "nlp"
  ],
  "author": "Warren & Claude",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micoverde/cultivate-uw-ml-mvp.git"
  }
}